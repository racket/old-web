    <title>PLT Docs</title>
    <description>Search the PLT Scheme Documentation</description>
    <site>http://docs.plt-scheme.org/</site>
    <option>
      <title>Search Context</title>
      <description>
        Enter a search context.
        For example, "M:" will look only for bindings, and "M:scheme"
        will look only for bindings from the scheme modules.
      </description>
      <default></default>
    </option>
    <search charset="UTF-8">http://docs.plt-scheme.org/search/search-context.html?q={query}&amp;hq={option1}</search>
    <send>http://docs.plt-scheme.org/search/search-context.html?q={selection}&amp;hq={option1}</send>
    <update>http://plt-scheme.org/search-button.xml</update>
    <gadget width="300" height="140"
            query="rawquery"
            selection="selectedtext"
            option1="option1">
